<%= render partial: 'errors', locals: { object: field_service } %>

<div class="form-group row">
  <%= form.label :name, class: 'col-md-2 col-form-label' %>
  <div class="col-md-4">
    <%= form.object.to_s %>
  </div>
</div>
<div class="form-group row">
  <%= form.label :field_service_operators, class: 'col-md-2 col-form-label' %>
  <div class="col-md-4">
    <%= form.select_many_field 'field_service_operator_ids',
          class: 'form-control', 'data-autocomplete-url': users_path, columns: 'name', object: User %>
  </div>
</div>
